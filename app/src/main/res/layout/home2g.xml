<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:background="@color/lxl_bg"
    android:orientation="vertical"
    tools:context=".Activity.H1.NewsFragment">

    <fragment
        android:id="@+id/frag"
        android:name="com.lecheng.hello.fzgjj.Activity.Unit.ActionBar"
        class="com.lecheng.hello.fzgjj.Activity.Unit.ActionBar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/pt48"
        tools:layout="@layout/unit_actionbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <TextView
            android:id="@+id/tv1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_blue"
            android:padding="@dimen/pt16"
            android:text="个人贷款办理信息"
            android:textColor="@color/colorWhite" />

        <TextView
            android:id="@+id/tv2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/pt1"
            android:background="@drawable/shape_blue"
            android:padding="@dimen/pt16"
            android:text="贷款账户基本信息"
            android:textColor="@color/colorWhite" />

        <TextView
            android:id="@+id/tv3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/pt1"
            android:background="@drawable/shape_blue"
            android:padding="@dimen/pt16"
            android:text="贷款还款业务信息"
            android:textColor="@color/colorWhite" />

        <TextView
            android:id="@+id/tv4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/pt1"
            android:background="@drawable/shape_blue"
            android:padding="@dimen/pt16"
            android:text="提取还款业务信息"
            android:textColor="@color/colorWhite" />
        <TextView
            android:id="@+id/tv5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/pt1"
            android:background="@drawable/shape_blue"
            android:padding="@dimen/pt16"
            android:text="贷款进度查询"
            android:textColor="@color/colorWhite" />

    </LinearLayout>


</LinearLayout>

