<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.lecheng.hello.fzgjj.Activity.H2.GJJAccountTQQuery">

    <fragment
        android:id="@+id/frag"
        android:name="com.lecheng.hello.fzgjj.Activity.Unit.ActionBar"
        class="com.lecheng.hello.fzgjj.Activity.Unit.ActionBar"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        tools:layout="@layout/unit_actionbar" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/colorTextGray2"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="@dimen/pt1"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="记账时间" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt72"
                    android:gravity="center"
                    android:padding="@dimen/fab_margin"
                    android:text="摘要" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="发生额(元)" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="还本金(元)"
                    android:textSize="13dp" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="还利息(元)" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="还欠本金(元)" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="还钱利息(元)" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="换催收利息(元)" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="提前还本金(元)" />

                <TextView
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="贷款余额(元)" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="@dimen/pt1"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv1"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

                <TextView
                    android:id="@+id/tv2"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt72"
                    android:gravity="center"
                    android:padding="@dimen/fab_margin"
                    android:text="..."
                    android:textSize="13dp" />

                <TextView
                    android:id="@+id/tv3"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

                <TextView
                    android:id="@+id/tv4"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

                <TextView
                    android:id="@+id/tv5"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

                <TextView
                    android:id="@+id/tv6"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

                <TextView
                    android:id="@+id/tv7"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

                <TextView
                    android:id="@+id/tv8"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

                <TextView
                    android:id="@+id/tv9"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

                <TextView
                    android:id="@+id/tv10"
                    style="@style/tvGrid"
                    android:layout_height="@dimen/pt36"
                    android:padding="@dimen/fab_margin"
                    android:text="..." />

            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
