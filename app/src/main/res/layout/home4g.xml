<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:background="@color/lxl_bg"
    android:orientation="vertical"
>

    <fragment
        android:id="@+id/frag"
        android:name="com.lecheng.hello.fzgjj.Activity.Unit.ActionBar"
        class="com.lecheng.hello.fzgjj.Activity.Unit.ActionBar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/pt48"
        android:layout_marginBottom="@dimen/fab_margin"
        tools:layout="@layout/unit_actionbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:padding="@dimen/fab_margin">

        <TextView
            android:id="@+id/tv1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/style_bg_white"
            android:padding="@dimen/fab_margin"
            android:text="第一个调查问卷"
            android:textSize="20dp" />

        <View style="@style/style_v_line1" />

        <TextView
            android:id="@+id/tv2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/style_bg_white"
            android:padding="@dimen/fab_margin"
            android:text="第二个调查问卷"
            android:textSize="20dp" />

        <View style="@style/style_v_line1" />

        <TextView
            android:id="@+id/tv3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/style_bg_white"
            android:padding="@dimen/fab_margin"
            android:text=""
            android:textSize="20dp" />

        <View style="@style/style_v_line1" />
    </LinearLayout>

</LinearLayout>
